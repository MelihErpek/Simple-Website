<body>
    <!-- NAVBAR -->
    <header> 
        <%- include ('partial/navbar.ejs') %>
    </header>           
    <main>
        <h1>Profil Sayfası</h1>
        <h2>
            <%=kisi %> adlı kişinin profilindesin.
            <p><%=kisi %> nin yasi <%=data.age %></p>
            <p><%=kisi %> nin isi <%=data.job %></p>
            <ul>
                <% data.langs.forEach((item)=>{  %>
                    <li>
                        <%=item %>
                    </li>
                <% }) %>
            </ul>
            <p>
                <%if(data.admin){   %>
                Admin sayfasındasın
                    
                <%}%>
            </p>
        </h2>
        
    </main>
</body>

</html>